html
  head
    title TreinaWeb
  body
  each val in data
    li
      div= val.nome
      div= val.categoria
      br
  br 
  br

  form(action="/" method="post")
    label Nome:
    input(
      type="text"
      name="nome"
      required
    )
    label Categoria:
    input(name="categoria" required)
      option Back-End
      option Desenvolvimento de Jogos
      option Front-End
      option Mobile
      input(
        type="submit"
        value="enviar"
      )